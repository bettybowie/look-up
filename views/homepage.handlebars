{{#each posts as |post|}}
<div class="center28">
  <div class="article-card28">
    <div class="content28">
      <p class="date28">Posted on: {{format_date date_created}}</p>
      <p class="title28">Posted by: {{user.username}}</p>
    </div>
    <div>
      <a href="{{file_URL}}" target="_blank">
      <img src="{{file_URL}}" alt="article-cover" width="450px" height="450px"/>
      </a>
    </div>
  </div>
    
</div>
{{/each}}